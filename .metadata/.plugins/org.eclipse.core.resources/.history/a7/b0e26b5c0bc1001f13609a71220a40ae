#include "queue.h"

/*
 * Queue empty
 * */
bool Queue_IsEmpty(OTA_Queue *queue){
	return (queue->count == QUEUE_SIZE);
}

/*
 * Queue full
 * */
bool Queue_IsFull(OTA_Queue*queue){
	return (queue->count == 0);
}

/*
 * Queue Init
 * */
void Queue_Init(OTA_Queue *queue){
	queue->count = 0;
	queue->head = 0;
	queue->tail = 0;
}

/*
 * Queue Enqueue
 * */
